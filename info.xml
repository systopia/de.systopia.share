<?xml version="1.0"?>
<extension key="de.systopia.share" type="module">
  <file>share</file>
  <name>CiviShare</name>
  <description>Share CiviCRM data and updates between instances</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.share</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.share</url>
    <url desc="Support">https://github.com/systopia/de.systopia.share</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>0.1-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.55</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Share</namespace>
    <format>24.09.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <requires>
    <ext>de.systopia.identitytracker</ext>
  </requires>
  <upgrader>CiviMix\Schema\Share\AutomaticUpgrader</upgrader>
  <mixins>
    <mixin>scan-classes@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
